import java.util.Scanner;

public class for1 {

    public static void main(String[] args) {
        //입력 스캐너생성
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        
        sc.close();
        
        
            //구구단이므로 9번씩 반복
            //n*1~ n*9까지 출력한다

            for(int i=1; i<10; i++) {
                
                System.out.println(N+"*"+i+"="+(N*i));
                
            }
            
        }
        

    }
